{
  "name": "flyapp",
  "version": "0.1.1",
  "author": {
    "name": "jackXu"
  },
  "description": "fast network transfer access tool",
  "private": true,
  "main": "app.js",
  "homepage": ".",
  "dependencies": {
    "@ant-design/icons": "^4.7.0",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.0.0",
    "@testing-library/user-event": "^13.2.1",
    "antd": "^4.19.3",
    "axios": "^0.26.0",
    "fs-extra": "^10.0.0",
    "koa": "^2.13.1",
    "koa-body": "^4.2.0",
    "koa-compose": "^4.1.0",
    "koa-router": "^10.0.0",
    "koa-static": "^5.0.0",
    "koa2-cors": "^2.0.6",
    "mime-types": "^2.1.35",
    "multiparty": "^4.2.3",
    "qr-image": "^3.2.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^6.2.2",
    "react-scripts": "5.0.0",
    "sqlite3": "^5.0.2",
    "web-vitals": "^2.1.0"
  },
  "devDependencies": {
    "concurrently": "^7.0.0",
    "electron": "18.0.2",
    "electron-builder": "^23.0.3",
    "nodemon": "^2.0.15",
    "wait-on": "^6.0.1"
  },
  "scripts": {
    "start": "BROWSER=none react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "postinstall": "electron-builder install-app-deps",
    "start:dev": "concurrently \"yarn start\" \"wait-on http://localhost:3000 && nodemon --watch app.js --watch win --watch server --exec 'yarn app-start:dev'\"",
    "start:build": "concurrently \"yarn build\" \"yarn pack:mac\"",
    "server:start": "NODE_ENV=serverDev nodemon server/app.js",
    "app-start": "NODE_ENV=prod electron .",
    "app-start:dev": "NODE_ENV=dev electron .",
    "pack:mac": "yarn build && electron-builder build --mac",
    "pack:win": "yarn build && electron-builder build --win --ia32"
  },
  "build": {
    "appId": "com.electron.flyapp",
    "productName": "flyapp",
    "asar": true,
    "files": [
      "app.js",
      "ipc.js",
      "win/*",
      "win/**/*",
      "build/*",
      "build/**/*",
      "server/*",
      "server/**/*",
      "src/preload/*"
    ],
    "extends": null,
    "extraFiles": [],
    "directories": {
      "buildResources": "static",
      "output": "release"
    },
    "copyright": "Copyright @ 2022 jackXu",
    "mac": {
      "target": [
        "dmg",
        "zip"
      ],
      "icon": "./src/assets/logo/logo.icns"
    },
    "win": {
      "target": [
        "nsis"
      ],
      "icon": "./src/assets/logo/logo.icns"
    }
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
