{
  "name": "flyphoto",
  "version": "0.1.0",
  "private": true,
  "main": "main.js",
  "homepage": ".",
  "dependencies": {
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.0.0",
    "@testing-library/user-event": "^13.2.1",
    "axios": "^0.26.0",
    "electron-squirrel-startup": "^1.0.0",
    "fs-extra": "^10.0.0",
    "koa": "^2.13.1",
    "koa-body": "^4.2.0",
    "koa-compose": "^4.1.0",
    "koa-router": "^10.0.0",
    "koa-static": "^5.0.0",
    "koa2-cors": "^2.0.6",
    "multiparty": "^4.2.3",
    "qr-image": "^3.2.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.6",
    "react-router-dom": "^6.2.2",
    "react-scripts": "5.0.0",
    "redux": "^4.1.2",
    "sqlite3": "^5.0.2",
    "web-vitals": "^2.1.0"
  },
  "devDependencies": {
    "concurrently": "^7.0.0",
    "electron": "16.0.4",
    "electron-packager": "^15.4.0",
    "nodemon": "^2.0.15",
    "wait-on": "^6.0.1"
  },
  "scripts": {
    "start": "BROWSER=none react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "start:dev":"concurrently \"yarn start\" \"wait-on http://localhost:3000 && yarn app-start:dev\"",
    "start:build":"concurrently \"yarn build\" \"yarn app-package:mac\"",
    "server:start": "NODE_ENV=serverDev nodemon server/app.js",
    "app-start": "NODE_ENV=prod electron .",
    "app-start:dev": "NODE_ENV=dev electron .",
    "app-package:mac": "electron-packager . --out ./out --platform=darwin --ignore=.git --icon=./src/assets/logo/logo.icns --asar",
    "app-package:win": "electron-packager . --out ./out --platform=win32 --ignore=.git --icon=./src/assets/logo/logo.ico --asar",
    "app-package:all": "electron-packager . --out ./out --all"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
